[Unit]
Description=Stackable Operator for Apache NiFi
Before=
After=network.target
[Service]
User=root
ExecStart=/opt/stackable/nifi-operator/stackable-nifi-operator
Restart=on-abort
StandardOutput=journal
StandardError=journal
Environment="NIFI_OPERATOR_LOG=info"
[Install]
WantedBy=multi-user.target
